									citycode = data.results[0]["address_components"][i]["long_name"];
							
									}
					       
					    		}
					    		
					    	
					
							}
							
							        
							        
							        input1=data.results[0].formatted_address;
							   		input1 = input1.split(",");							
									var secondline=''
									
									for(i=1; i< (input1.length-1); i++)
